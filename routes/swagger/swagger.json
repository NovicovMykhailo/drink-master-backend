							}
						}
					}
				},
				"responses": {
					"200": {
						"description": "Subscribe email sent",
						"content": {
							"application/json": {
								"schema": {
									"example": {
										"message": "Subscribe email sent"
									}
								}
							}
						}
					},
					"400": {
						"description": "Bad request (invalid request body)",
						"content": {}
					},
					"401": {
						"description": "Unauthorized: Not authorized"
					},
					"403": {
						"description": "Forbidden"
					},
					"409": {
						"description": "Already subscribed to this email",
						"content": {}
					},
					"500": {
						"description": "Internal Server Error"
					}
				}
			}
